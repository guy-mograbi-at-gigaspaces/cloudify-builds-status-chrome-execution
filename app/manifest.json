{
    "name": "__MSG_appName__",
    "version": "0.0.267",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "images/cloudify-logo-2.png",
        "128": "images/cloudify-logo-2.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "bower_components/lodash/lodash.js",
            "bower_components/jquery/dist/jquery.js",
            "scripts/chromereload.js",
            "scripts/travis.js",
            "scripts/config.js",
            "scripts/background.js"
        ]
    },
    "permissions": [
        "storage"
    ],
    "options_page": "options.html",
    "browser_action": {
        "default_icon": {
            "16": "images/cloudify-logo-2.png",
            "19": "images/cloudify-logo-2.png",
            "38": "images/cloudify-logo-2.png",
            "128": "images/cloudify-logo-2.png"
        },
        "default_title": "my first chrome extension",
        "default_popup": "popup.html"
    }
}